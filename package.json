{
  "name": "base-test-suite",
  "description": "Test suite for base projects.",
  "version": "0.1.12",
  "homepage": "https://github.com/node-base/base-test-suite",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "node-base/base-test-suite",
  "bugs": {
    "url": "https://github.com/node-base/base-test-suite/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "lib",
    "suite"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "assemble-fs": "^0.6.0",
    "async-each": "^1.0.0",
    "buffer-equal": "^1.0.0",
    "consolidate": "^0.14.1",
    "define-property": "^0.2.5",
    "engine-base": "^0.1.2",
    "engine-handlebars": "^0.8.0",
    "event-stream": "^3.3.4",
    "expect": "^1.20.2",
    "find-pkg": "^0.1.2",
    "get-value": "^2.0.6",
    "graceful-fs": "^4.1.4",
    "gulp-format-md": "^0.1.9",
    "is-buffer": "^1.1.3",
    "kind-of": "^3.0.3",
    "load-pkg": "^3.0.1",
    "parser-front-matter": "^1.4.1",
    "resolve-glob": "^0.1.8",
    "rimraf": "^2.5.3",
    "sinon": "^1.17.4",
    "swig": "^1.4.2",
    "through2": "^2.0.1",
    "vinyl": "^1.1.1"
  },
  "devDependencies": {
    "assemble-core": "^0.25.0",
    "base-test-runner": "^0.2.0",
    "gulp": "^3.9.1",
    "mocha": "^2.5.3",
    "templates": "^0.24.2",
    "yargs-parser": "^2.4.1"
  },
  "keywords": [
    "base",
    "suite",
    "test",
    "tests",
    "unit"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "assemble",
        "base",
        "generate",
        "verb"
      ]
    },
    "reflinks": [
      "assemble",
      "base",
      "generate",
      "templates",
      "verb",
      "verb-readme-generator"
    ],
    "lint": {
      "reflinks": true
    }
  },
  "lintDeps": {
    "dependencies": [
      "lib/lookup.js",
      "suite/**/*.js"
    ],
    "devDependencies": [
      "lib/converter.js",
      "test/*.js"
    ]
  }
}
