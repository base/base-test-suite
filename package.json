{
  "name": "base-test-suite",
  "description": "Test suite for base projects.",
  "version": "0.3.2",
  "homepage": "https://github.com/node-base/base-test-suite",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "node-base/base-test-suite",
  "bugs": {
    "url": "https://github.com/node-base/base-test-suite/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "lib",
    "suite"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "assemble-core": "^0.26.0",
    "assemble-fs": "^0.8.0",
    "async-each": "^1.0.1",
    "base-test-runner": "^0.2.0",
    "buffer-equal": "^1.0.0",
    "consolidate": "^0.14.5",
    "define-property": "^0.2.5",
    "engine-base": "^0.1.2",
    "engine-handlebars": "^0.8.0",
    "event-stream": "^3.3.4",
    "expect": "^1.20.2",
    "find-pkg": "^0.1.2",
    "get-value": "^2.0.6",
    "graceful-fs": "^4.1.11",
    "is-buffer": "^1.1.4",
    "kind-of": "^3.1.0",
    "load-pkg": "^3.0.1",
    "parser-front-matter": "^1.6.2",
    "resolve-glob": "^0.1.8",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.7",
    "swig": "^1.4.2",
    "templates": "^0.25.3",
    "through2": "^2.0.3",
    "vinyl": "^2.0.1",
    "yargs-parser": "^4.2.1"
  },
  "keywords": [
    "api",
    "app",
    "application",
    "base",
    "baseplugin",
    "building-blocks",
    "create",
    "framework",
    "plugin",
    "plugins",
    "suite",
    "test",
    "tests",
    "tool",
    "toolkit",
    "tools",
    "unit"
  ],
  "lintDeps": {
    "defaultIgnores": [
      "**/node_modules/**",
      "**/fixtures/**"
    ],
    "files": {
      "dependencies": [
        "index.js",
        "lib/**/*.js",
        "suite/{support,test}/**/*.js",
        "test/**/*.js"
      ],
      "devDependencies": []
    }
  },
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "assemble",
        "base",
        "generate",
        "verb"
      ]
    },
    "reflinks": [
      "assemble",
      "base",
      "generate",
      "templates",
      "verb",
      "verb-readme-generator"
    ],
    "lint": {
      "reflinks": true
    }
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-format-md": "^0.1.11"
  }
}
